<p id="notice"><%= notice %></p>

<h1>Paypal Payments</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>

    <% @payment_history.payments.each do |payment| %>
      <tr>
        <td><%= payment %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<%= link_to 'Check Out', checkout_path %>
